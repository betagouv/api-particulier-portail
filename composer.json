{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "alterphp/easyadmin-extension-bundle": "^3.0",
    "easycorp/easyadmin-bundle": "^2.3",
    "hautelook/alice-bundle": "^2.7",
    "influxdb/influxdb-php": "^1.15",
    "knpuniversity/oauth2-client-bundle": "^2.0",
    "nyholm/psr7": "^1.2",
    "predis/predis": "^1.1",
    "ramsey/uuid-doctrine": "^1.6",
    "sensio/framework-extra-bundle": "^5.5",
    "stof/doctrine-extensions-bundle": "^1.4",
    "symfony/console": "5.0.*",
    "symfony/dotenv": "5.0.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.0.*",
    "symfony/http-client": "5.0.*",
    "symfony/monolog-bundle": "^3.5",
    "symfony/orm-pack": "^1.0",
    "symfony/serializer": "5.0.*",
    "symfony/twig-pack": "^1.0",
    "symfony/yaml": "5.0.*",
    "trikoder/oauth2-bundle": "^3.0"
  },
  "require-dev": {
    "easycorp/easy-deploy-bundle": "^1.0",
    "symfony/browser-kit": "5.0.*",
    "symfony/debug-bundle": "5.0.*",
    "symfony/maker-bundle": "^1.14",
    "symfony/phpunit-bridge": "^5.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/var-dumper": "5.0.*",
    "theofidry/psysh-bundle": "^4.3"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "doctrine:migrations:migrate --no-interaction": "symfony-cmd",
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  }
}
